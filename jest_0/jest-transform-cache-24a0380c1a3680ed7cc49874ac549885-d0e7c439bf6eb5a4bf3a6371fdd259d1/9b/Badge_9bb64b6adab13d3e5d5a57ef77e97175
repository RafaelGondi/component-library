  computed: {
    predefinedStyle() {
      let computed_style = '';

      switch (this.variant) {
        case 'yellow':
          computed_style = 'yellow';
          break;

        case 'green':
          computed_style = 'green';
          break;

        case 'red':
          computed_style = 'red';
          break;

        case 'blue':
          computed_style = 'blue';
          break;

        case 'purple':
          computed_style = 'purple';
          break;

        case 'gray':
          computed_style = 'gray';
          break;

        default:
          computed_style = 'yellow';
          break;
      }

      return computed_style;
    },

