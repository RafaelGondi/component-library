"use strict";

var _interopRequireDefault = require("/home/sysvale/component-library/node_modules/@babel/runtime/helpers/interopRequireDefault");

var _testUtils = require("@vue/test-utils");

var _bootstrapVue = _interopRequireDefault(require("bootstrap-vue"));

var _Pagination = _interopRequireDefault(require("../../src/components/Pagination.vue"));

const localVue = (0, _testUtils.createLocalVue)();
localVue.use(_bootstrapVue.default);
test('Component is mounted properly', () => {
  const wrapper = (0, _testUtils.mount)(_Pagination.default, {
    localVue
  });
  expect(wrapper).toMatchSnapshot();
});
test('Click event is recognized', () => {
  const wrapper = (0, _testUtils.mount)(_Pagination.default, {
    localVue,
    propsData: {
      perPage: 2,
      totalRows: 6,
      activeBgColor: '#545b62',
      activeTextColor: '#FFFFFF',
      hoverBgColor: '#545b62',
      hoverTextColor: '#FFFFFF',
      borderRadius: 5
    }
  });
  wrapper.findAll('.pagination-button').at(0).trigger('click');
  expect(wrapper.emitted().input).toBeTruthy();
});
test('Page number is correct', () => {
  const wrapper = (0, _testUtils.mount)(_Pagination.default, {
    localVue,
    propsData: {
      perPage: 10,
      totalRows: 21,
      activeBgColor: '#545b62',
      activeTextColor: '#FFFFFF',
      hoverBgColor: '#545b62',
      hoverTextColor: '#FFFFFF',
      borderRadius: 5
    }
  });
  expect(wrapper.findAll('.pagination-button').length).toBe(3);
});
